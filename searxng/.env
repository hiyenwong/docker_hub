# External port exposed on host
HOST_PORT=8083

# Access / security
# Required: change to a long random string
SEARXNG_SECRET=change-me-please

# Base URL users will access (no trailing slash)
SEARXNG_BASE_URL=http://localhost:8080

# Rate limiting / access control backend
SEARXNG_VALKEY_URL=valkey://valkey:6379/0

# Basic auth for nginx (set to "off" to disable)
BASIC_AUTH_REALM=Restricted
